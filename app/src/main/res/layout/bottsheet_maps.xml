<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="horizontal"
    android:weightSum="5">

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="18.9dp"
        android:layout_marginTop="15dp"
        android:layout_marginBottom="15dp"
        android:layout_weight="3"
        android:orientation="vertical">

        <TextView
            android:id="@+id/tv_fran_name"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="gs25 얘미뇽아"
            android:textSize="20dp" />

        <TextView
            android:id="@+id/tv_fran_type"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="펴릐럼"
            android:textSize="12dp" />

        <TextView
            android:id="@+id/tv_fran_addr"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="돈암동 어쩌구저쩌구"
            android:textSize="12dp" />

        <TextView
            android:id="@+id/tv_fran_num"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="02-0000-0000"
            android:textSize="12dp" />
    </LinearLayout>

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:layout_weight="2"
        android:layout_marginRight="18.9dp"
        android:layout_marginTop="15dp"
        android:layout_marginBottom="15dp"
        app:srcCompat="@drawable/colormap_dongdaemoon" />
</LinearLayout>