<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.eco.ecoseoul">

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/Theme.AppCompat.NoActionBar">

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/google_maps_key" />

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".home.view.LoginActivity" />
        <activity android:name=".home.view.AllActivity" />
        <activity android:name=".community.CommunityActivity" />
        <activity android:name=".community.CommunityDetailActivity" />
        <activity android:name=".community.CommunityWriteActivity" />
        <activity android:name=".RegistrationActivity"></activity>

        <activity android:name=".SearchFranActivity" />
        <activity android:name=".MapsActivity" />

    </application>

</manifest>